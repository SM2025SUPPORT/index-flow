{
  "name": "index-flow",
  "version": "1.0.0",
  "description": "1-click auto-indexing for Webflow - submits content to GSC + IndexNow automatically",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "setup": "node src/scripts/setup.js",
    "build": "npm install --build-from-source"
  },
  "keywords": [
    "webflow",
    "indexing",
    "seo",
    "indexnow",
    "google-search-console"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "better-sqlite3": "^9.2.2",
    "googleapis": "^129.0.0",
    "uuid": "^9.0.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
